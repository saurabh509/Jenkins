         agent any
         stages {
                 stage('Clone') {
                 steps {
                    echo('Sample testing of Stage 1')
                 }
                 }
                 stage('Build') {
                 steps {
                    echo('Sample testing of Stage 2')
                 }
                 }
                 stage('Deploy') {
                
                 steps {
                       echo 'Thanks for using Jenkins Pipeline'
                 }
                 }
              }
}
